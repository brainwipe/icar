                <ITEXT CH="Vitero: Rim 4, Spin 47"/>
                <para PARENT="Body Text"/>
                <ITEXT CH="Turus: Hub 1, Spin 41"/>
                <para PARENT="Body Text"/>
                <ITEXT CH="Romar: Hub 6, Spin 54"/>
                <para PARENT="Body Text"/>
                <ITEXT CH="Niopak: Hub 3, Spin 54 Middle"/>
                <para PARENT="Body Text"/>
                <ITEXT CH="Naggachef: Rim 2, Spin 56 Middle"/>
                <para PARENT="Body Text"/>
                <ITEXT CH="Dorian Sector"/>
